{
    "manifest_version" : 2,
    "name" : "OdisseyChromeExtension",
    "description" : "Reproductor de musica youtube",
    "version" : "0.3",
    "icons": { "16": "/images/pixel-icon.png"},
    "browser_action" : {
        "default_popup" : "index.html",
        "default_title" : "odissey"
    },
    "background" :{
        "page":"background.html"
    },
    "omnibox": { "keyword" : "@odyssey" },
   
    "content_security_policy": "script-src 'self' https://www.youtube.com/iframe_api https://www.youtube.com/s/player/bfb74eaf/www-widgetapi.vflset/www-widgetapi.js; object-src 'self'",
    
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "http://*/*" , "https://*/*",
        "cookies",
        "identity",
        "identity.email"
    ]
}